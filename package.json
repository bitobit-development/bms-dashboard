{
  "name": "bms-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:generate": "dotenv -e .env.local -- drizzle-kit generate",
    "db:push": "dotenv -e .env.local -- drizzle-kit push",
    "db:migrate": "dotenv -e .env.local -- drizzle-kit migrate",
    "db:studio": "dotenv -e .env.local -- drizzle-kit studio",
    "db:drop": "dotenv -e .env.local -- drizzle-kit drop",
    "db:seed": "dotenv -e .env.local -- tsx src/db/seed.ts",
    "db:seed:weather": "dotenv -e .env.local -- tsx src/db/seed-weather.ts",
    "db:seed:harry-gwala": "dotenv -e .env.local -- tsx src/scripts/seed-harry-gwala.ts",
    "weather:test": "dotenv -e .env.local -- tsx src/scripts/test-weather.ts",
    "telemetry:run": "dotenv -e .env.local -- tsx src/scripts/run-telemetry-generator.ts",
    "telemetry:1min": "dotenv -e .env.local -- tsx src/scripts/run-telemetry-generator.ts 1",
    "telemetry:5min": "dotenv -e .env.local -- tsx src/scripts/run-telemetry-generator.ts 5",
    "telemetry:check": "dotenv -e .env.local -- tsx src/scripts/check-telemetry.ts",
    "db:stats": "dotenv -e .env.local -- tsx src/scripts/check-database-stats.ts",
    "telemetry:pm2:start": "pm2 start ecosystem.config.js",
    "telemetry:pm2:stop": "pm2 stop bms-telemetry-generator",
    "telemetry:pm2:restart": "pm2 restart bms-telemetry-generator",
    "telemetry:pm2:logs": "pm2 logs bms-telemetry-generator",
    "telemetry:pm2:status": "pm2 status bms-telemetry-generator",
    "user:approve-admin": "dotenv -e .env.local -- tsx scripts/approve-admin.ts"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@stackframe/stack": "^2.8.47",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.7",
    "lucide-react": "^0.548.0",
    "next": "16.0.1",
    "next-themes": "^0.4.6",
    "postgres": "^3.4.7",
    "react": "19.2.0",
    "react-day-picker": "^9.11.1",
    "react-dom": "19.2.0",
    "recharts": "^3.3.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.6",
    "eslint": "^9",
    "eslint-config-next": "16.0.1",
    "tailwindcss": "^4",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5"
  }
}
